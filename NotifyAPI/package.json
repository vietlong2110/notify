{
  "name": "notifyapi",
  "version": "1.0.0",
  "description": "notification app",
  "main": "app.js",
  "scripts": {
    "start:mongo": "docker-compose -f dev.yml start mongo",
    "stop:mongo": "docker-compose -f dev.yml stop mongo",
    "start:elastic": "docker-compose -f dev.yml start elasticsearch",
    "stop:elastic": "docker-compose -f dev.yml stop elasticsearch",
    "start": "pm2 start app.js",
    "stop": "pm2 stop app.js",
    "start:index": "pm2 start indexing.js",
    "stop:index": "pm2 stop indexing.js",
    "log:elastic": "docker-compose -f dev.yml logs -f --tail 20 elastic",
    "log:notify": "docker-compose -f dev.yml logs -f --tail 20 notify",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "notification",
    "news",
    "media"
  ],
  "author": "Long Le",
  "license": "MIT",
  "dependencies": {
    "bella-date": "^1.1.2",
    "bellajs": "^7.1.2",
    "bluebird": "^3.5.0",
    "body-parser": "^1.17.2",
    "cookie-parser": "^1.4.3",
    "diacritic": "^0.0.2",
    "elasticsearch": "^13.1.1",
    "express": "^4.15.3",
    "fb": "^2.0.0",
    "friendly-xml": "^1.0.1",
    "generate-password": "^1.3.0",
    "html-entities": "^1.2.1",
    "isomorphic-fetch": "^2.2.1",
    "jwt-simple": "^0.5.1",
    "lodash": "^4.17.4",
    "mongoosastic": "^4.3.0",
    "mongoose": "^4.10.7",
    "node-cache": "^4.1.1",
    "passport": "^0.3.2",
    "passport-jwt": "^2.2.1",
    "socket.io": "^2.0.3",
    "unfluff": "1.1.0",
    "url": "^0.11.0",
    "xml2json": "^0.11.0"
  },
  "devDependencies": {
    "chai": "*",
    "mocha": "^3.4.2"
  }
}
